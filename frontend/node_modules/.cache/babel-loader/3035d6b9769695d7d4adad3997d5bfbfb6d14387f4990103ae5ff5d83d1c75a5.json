{"ast":null,"code":"import AppHeader from './AppHeader.vue';\nexport default {\n  name: 'EventDetails',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      eventDetails: {},\n      schedule: []\n    };\n  },\n  methods: {\n    getMatchType(matchNumber) {\n      switch (matchNumber) {\n        case 1:\n          return \"Prelim\";\n        case 2:\n          return \"Quarters\";\n        case 3:\n          return \"Semi\";\n        case 4:\n          return \"Finals\";\n        default:\n          return \"Unknown\";\n      }\n    },\n    getScoreStyle(scheduleItem) {\n      const homeScore = scheduleItem.homeTeamPoints;\n      const awayScore = scheduleItem.awayTeamPoints;\n      if (homeScore > awayScore) {\n        return {\n          color: 'green'\n        }; // Home team won, green text\n      } else if (homeScore < awayScore) {\n        return {\n          color: 'red'\n        }; // Away team won, red text\n      } else {\n        return {\n          color: 'black'\n        }; // Draw, default color\n      }\n    }\n  },\n  mounted() {\n    const eventId = this.$route.params.id;\n\n    // Fetch event details based on the ID\n    fetch(`http://localhost:8080/events/${eventId}`).then(response => response.json()).then(data => {\n      this.eventDetails = data;\n    });\n\n    // Fetch schedule based on the event ID\n    fetch(`http://localhost:8080/schedule/${eventId}`).then(response => response.json()).then(data => {\n      // Add matchNumber property to each schedule item\n      this.schedule = data.map(scheduleItem => ({\n        ...scheduleItem,\n        matchNumber: scheduleItem.matchNumber\n      }));\n    });\n  }\n};","map":{"version":3,"names":["AppHeader","name","components","data","eventDetails","schedule","methods","getMatchType","matchNumber","getScoreStyle","scheduleItem","homeScore","homeTeamPoints","awayScore","awayTeamPoints","color","mounted","eventId","$route","params","id","fetch","then","response","json","map"],"sources":["C:\\Users\\Kya\\Desktop\\123\\frontend\\src\\views\\EventDetails.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <App-header></App-header>\r\n    <h1>{{ eventDetails.eventName }}</h1>\r\n    <p>Start Date: {{ eventDetails.eventStart }}</p>\r\n    <p>End Date: {{ eventDetails.eventEnd }}</p>\r\n    <!-- Display schedule here -->\r\n    <h2>Schedule</h2>\r\n    <ul>\r\n      <li v-for=\"scheduleItem in schedule\" :key=\"scheduleItem.scheduleId\" :style=\"getScoreStyle(scheduleItem)\">\r\n        <!-- Display match type, teams, and points -->\r\n        {{ getMatchType(scheduleItem.matchNumber) }}: \r\n        {{ scheduleItem.homeTeamName }} ({{ scheduleItem.homeTeamPoints }}) vs \r\n        {{ scheduleItem.awayTeamName }} ({{ scheduleItem.awayTeamPoints }})\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppHeader from './AppHeader.vue';\r\n\r\nexport default {\r\n  name: 'EventDetails',\r\n  components: {\r\n    AppHeader,\r\n  },\r\n  data() {\r\n    return {\r\n      eventDetails: {},\r\n      schedule: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getMatchType(matchNumber) {\r\n      switch (matchNumber) {\r\n        case 1:\r\n          return \"Prelim\";\r\n        case 2:\r\n          return \"Quarters\";\r\n        case 3:\r\n          return \"Semi\";\r\n        case 4:\r\n          return \"Finals\";\r\n        default:\r\n          return \"Unknown\";\r\n      }\r\n    },\r\n    getScoreStyle(scheduleItem) {\r\n      const homeScore = scheduleItem.homeTeamPoints;\r\n      const awayScore = scheduleItem.awayTeamPoints;\r\n\r\n      if (homeScore > awayScore) {\r\n        return { color: 'green' }; // Home team won, green text\r\n      } else if (homeScore < awayScore) {\r\n        return { color: 'red' }; // Away team won, red text\r\n      } else {\r\n        return { color: 'black' }; // Draw, default color\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    const eventId = this.$route.params.id;\r\n\r\n    // Fetch event details based on the ID\r\n    fetch(`http://localhost:8080/events/${eventId}`)\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        this.eventDetails = data;\r\n      });\r\n\r\n    // Fetch schedule based on the event ID\r\n    fetch(`http://localhost:8080/schedule/${eventId}`)\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        // Add matchNumber property to each schedule item\r\n        this.schedule = data.map((scheduleItem) => ({\r\n          ...scheduleItem,\r\n          matchNumber: scheduleItem.matchNumber,\r\n        }));\r\n      });\r\n  },\r\n};\r\n</script>"],"mappings":"AAoBA,OAAOA,SAAQ,MAAO,iBAAiB;AAEvC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC,CAAC;MAChBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,WAAW,EAAE;MACxB,QAAQA,WAAW;QACjB,KAAK,CAAC;UACJ,OAAO,QAAQ;QACjB,KAAK,CAAC;UACJ,OAAO,UAAU;QACnB,KAAK,CAAC;UACJ,OAAO,MAAM;QACf,KAAK,CAAC;UACJ,OAAO,QAAQ;QACjB;UACE,OAAO,SAAS;MACpB;IACF,CAAC;IACDC,aAAaA,CAACC,YAAY,EAAE;MAC1B,MAAMC,SAAQ,GAAID,YAAY,CAACE,cAAc;MAC7C,MAAMC,SAAQ,GAAIH,YAAY,CAACI,cAAc;MAE7C,IAAIH,SAAQ,GAAIE,SAAS,EAAE;QACzB,OAAO;UAAEE,KAAK,EAAE;QAAQ,CAAC,EAAE;MAC7B,OAAO,IAAIJ,SAAQ,GAAIE,SAAS,EAAE;QAChC,OAAO;UAAEE,KAAK,EAAE;QAAM,CAAC,EAAE;MAC3B,OAAO;QACL,OAAO;UAAEA,KAAK,EAAE;QAAQ,CAAC,EAAE;MAC7B;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;;IAErC;IACAC,KAAK,CAAE,gCAA+BJ,OAAQ,EAAC,EAC5CK,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEnB,IAAI,IAAK;MACd,IAAI,CAACC,YAAW,GAAID,IAAI;IAC1B,CAAC,CAAC;;IAEJ;IACAkB,KAAK,CAAE,kCAAiCJ,OAAQ,EAAC,EAC9CK,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEnB,IAAI,IAAK;MACd;MACA,IAAI,CAACE,QAAO,GAAIF,IAAI,CAACsB,GAAG,CAAEf,YAAY,KAAM;QAC1C,GAAGA,YAAY;QACfF,WAAW,EAAEE,YAAY,CAACF;MAC5B,CAAC,CAAC,CAAC;IACL,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependen