{"ast":null,"code":"import EventDetails from './EventDetails.vue';\nexport default {\n  name: 'HelloWorld',\n  components: {\n    EventDetails\n  },\n  data() {\n    return {\n      teams: [],\n      events: [],\n      selectedEvent: null\n    };\n  },\n  methods: {\n    showEventDetails(event) {\n      this.selectedEvent = event;\n    }\n  },\n  mounted() {\n    // Fetch teams\n    fetch(\"http://localhost:8080/team\").then(response => response.json()).then(data => {\n      this.teams = data.map(item => item.team);\n    });\n\n    // Fetch events\n    fetch(\"http://localhost:8080/events\").then(response => response.json()).then(data => {\n      // Update this part to include the id when creating Event instances\n      this.events = data.map(event => ({\n        id: event.eventID,\n        eventName: event.eventName,\n        eventStart: event.eventStart,\n        eventEnd: event.eventEnd\n      })).sort((a, b) => new Date(b.eventStart) - new Date(a.eventStart));\n    });\n  }\n};","map":{"version":3,"names":["EventDetails","name","components","data","teams","events","selectedEvent","methods","showEventDetails","event","mounted","fetch","then","response","json","map","item","team","id","eventID","eventName","eventStart","eventEnd","sort","a","b","Date"],"sources":["C:\\Users\\Kya\\Desktop\\123\\frontend\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"logo\">\n      <img src=\"../pictures/ShotCounterLOGO.png\" alt=\"Logo\" class =\"logo-img\">\n      <div class = \"navbar\">\n        <ul>\n        <li><a href=\"#\">Home</a></li>\n        <li><a href=\"#\">Events</a></li>\n        <li><a href=\"#\">Blog</a></li>\n        </ul>\n      </div>\n      <!--end nav -->\n    </div>\n  </div>\n  <div class=\"imgcontainer\">\n    <img src=\"../pictures/ShotCounterFrontPage.png\" alt=\"Logo\" class =\"logo-img1\">\n  </div>\n  <div class=\"content\" >\n    <h2>STAT TRACKING</h2>\n    <h3>For NXL</h3>\n  </div>\n  <div class=\"Teamstuff\">\n    <h1>Team</h1>\n    <ul class=\"team-list\">\n      <li v-for=\"(teamName, index) in teams\" :key=\"index\">{{ teamName }}</li>\n    </ul>\n  </div>\n  <div class=\"EventList\">\n    <h1>Events</h1>\n    <ul class=\"event-list\">\n      <li v-for=\"(event, index) in events\" :key=\"index\">\n  <router-link :to=\"{ name: 'EventDetails', params: { id: event.id }}\">\n    <strong @click=\"showEventDetails(event)\">{{ event.eventName }}</strong>\n  </router-link>\n  <span class=\"event-start-date\">Start Date \"{{ event.eventStart }}\"</span>\n  <span class=\"event-end-date\">End Date \"{{ event.eventEnd }}\"</span>\n</li>\n    </ul>\n  </div>\n\n  <div v-if=\"selectedEvent\">\n    <EventDetails :event=\"selectedEvent\" />\n  </div>\n</template>\n\n<script>\nimport EventDetails from './EventDetails.vue';\nexport default {\n  name: 'HelloWorld',\n  components: {\n    EventDetails,\n  },\n  data() {\n    return {\n      teams: [],\n      events: [],\n      selectedEvent: null,\n    };\n  },\n  methods: {\n    showEventDetails(event) {\n      this.selectedEvent = event;\n    },\n  },\n  mounted() {\n    // Fetch teams\n    fetch(\"http://localhost:8080/team\")\n      .then((response) => response.json())\n      .then((data) => {  \n        this.teams = data.map((item) => item.team);\n      });\n\n    // Fetch events\n    fetch(\"http://localhost:8080/events\")  \n      .then((response) => response.json())\n      .then((data) => {\n        // Update this part to include the id when creating Event instances\n        this.events = data.map((event) => ({\n          id: event.eventID,\n          eventName: event.eventName,\n          eventStart: event.eventStart,\n          eventEnd: event.eventEnd\n        })).sort((a, b) => new Date(b.eventStart) - new Date(a.eventStart));\n      });\n  }\n};\n</script>\n<style>\n\n*{\n  margin: 0;\n}\n\n\n.container {\n  position: relative;\n  height: 80px;\n}\n\n.logo-img {\n  width: 400px;\n  position: absolute;\n  top: 0;\n  left: 10px;\n  z-index: 998;\n}\n\n.imgcontainer {\n  position: absolute;\n  top: 37%;\n  right: 5%;\n  transform: translateY(-50%); \n  z-index: 999;\n  right: 304px;\n  bottom: 10px\n}\n\n.logo-img1 {\n  transform: scaleX(-1);\n  width: 700px; \n}\n\n.navbar {\n  height: 90px;\n  width: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  display: grid;\n  background: rgb(30, 160, 247);\n  place-items: center;\n}\n\n.navbar ul{\n  list-style: none;\n  display: flex;\n  flex-direction: row;\n  z-index: 1;\n}\n\n.navbar ul li{\n  font-size: 20px;\n  margin: 0 45px;\n  z-index: 1;\n}\n\n\n.navbar ul li a{\n  text-decoration: none;\n  color: black;\n}\n\n.navbar ul li a:hover{\n  color: rgb(255, 255, 255)\n}\n\n.content{\n  position: absolute;\n  top: 34%;\n  left: 9%;\n  z-index: 996;\n}\n.content:hover h2{\n  color: rgb(121, 45, 192);\n  transition: 0.4s ease-in-out;\n}\n\n\n.content:hover h3{\n  color: rgb(121, 45, 192);\n  transition: 1.4s ease-in-out;\n}\n\n.content h3{\n  font-size: 62px;\n  margin-top: 20px;\n}\n\n.Teamstuff {\n  position: absolute;\n  left: 240px;\n  top: 600px;\n}\n.EventList {\n  position: absolute;\n  top: 557px;\n  right: 200px;\n  border: 2px solid #000;\n  padding: 10px;\n  border-radius: 5px;\n  background-color: #fff;\n  width: 900px; /* Set your desired width */\n}\n\n/* Add styles for the EventList header */\n.EventList h1 {\n  font-size: 50px;\n  font-weight: bold;\n  margin-bottom: 10px;\n  text-align: center; /* Space between header and events */\n}\n\n/* Add styles for the event list items */\n.EventList ul {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n  font-size: 18px;\n  text-align: center;\n  position: relative;\n}\n\n.EventList li {\n  margin-bottom: 7px; \n}\n\n.EventList li .event-start-date {\n  font-size: 16px;\n  color: green; \n  margin-left: 8px; \n}\n\n/* Style the end date */\n.EventList li .event-end-date {\n  font-size: 16px;\n  color: red;\n  margin-left: 8px; \n}\n\n\ncontent.h2 {\n    color: #131313;\n    background-color: #e7e5e4;\n    letter-spacing: .15em;\n    text-shadow: \n      1px -1px 0 #767676, \n      -1px 2px 1px #737272, \n      -2px 4px 1px #767474, \n      -3px 6px 1px #787777, \n      -4px 8px 1px #7b7a7a, \n      -5px 10px 1px #7f7d7d, \n      -6px 12px 1px #828181, \n      -7px 14px 1px #868585, \n      -8px 16px 1px #8b8a89, \n      -9px 18px 1px #8f8e8d, \n      -10px 20px 1px #949392, \n      -11px 22px 1px #999897, \n      -12px 24px 1px #9e9c9c, \n      -13px 26px 1px #a3a1a1, \n      -14px 28px 1px #a8a6a6, \n      -15px 30px 1px #adabab, \n      -16px 32px 1px #b2b1b0, \n      -17px 34px 1px #b7b6b5, \n      -18px 36px 1px #bcbbba, \n      -19px 38px 1px #c1bfbf, \n      -20px 40px 1px #c6c4c4, \n      -21px 42px 1px #cbc9c8, \n      -22px 44px 1px #cfcdcd, \n      -23px 46px 1px #d4d2d1, \n      -24px 48px 1px #d8d6d5, \n      -25px 50px 1px #dbdad9, \n      -26px 52px 1px #dfdddc, \n      -27px 54px 1px #e2e0df, \n      -28px 56px 1px #e4e3e2;\n  }\n</style>"],"mappings":"AA8CA,OAAOA,YAAW,MAAO,oBAAoB;AAC7C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACH,aAAY,GAAIG,KAAK;IAC5B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAC,KAAK,CAAC,4BAA4B,EAC/BC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAET,IAAI,IAAK;MACd,IAAI,CAACC,KAAI,GAAID,IAAI,CAACY,GAAG,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAI,CAAC;IAC5C,CAAC,CAAC;;IAEJ;IACAN,KAAK,CAAC,8BAA8B,EACjCC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAET,IAAI,IAAK;MACd;MACA,IAAI,CAACE,MAAK,GAAIF,IAAI,CAACY,GAAG,CAAEN,KAAK,KAAM;QACjCS,EAAE,EAAET,KAAK,CAACU,OAAO;QACjBC,SAAS,EAAEX,KAAK,CAACW,SAAS;QAC1BC,UAAU,EAAEZ,KAAK,CAACY,UAAU;QAC5BC,QAAQ,EAAEb,KAAK,CAACa;MAClB,CAAC,CAAC,CAAC,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK,IAAIC,IAAI,CAACD,CAAC,CAACJ,UAAU,IAAI,IAAIK,IAAI,CAACF,CAAC,CAACH,UAAU,CAAC,C